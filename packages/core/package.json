{
    "name": "@api-forge/core",
    "version": "0.1.0",
    "description": "Core API Forge library - OAuth, rate limiting, docs, and portal",
    "author": "Diluk Angelo <dilukangelo@gmail.com>",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./storage/memory": {
            "import": "./dist/storage/memory.js",
            "types": "./dist/storage/memory.d.ts"
        },
        "./storage/redis": {
            "import": "./dist/storage/redis.js",
            "types": "./dist/storage/redis.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "bun build ./src/index.ts --outdir ./dist --target bun && tsc --emitDeclarationOnly --declaration --declarationDir ./dist",
        "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir ./dist",
        "dev": "bun --watch ./src/index.ts",
        "test": "bun test"
    },
    "dependencies": {
        "argon2": "^0.44.0",
        "jose": "^5.2.0",
        "zod": "^3.22.0"
    },
    "peerDependencies": {
        "ioredis": "^5.0.0"
    },
    "peerDependenciesMeta": {
        "ioredis": {
            "optional": true
        }
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/express": "^4.17.21",
        "bun-types": "^1.3.8",
        "express": "^4.18.0",
        "typescript": "^5.4.0"
    }
}